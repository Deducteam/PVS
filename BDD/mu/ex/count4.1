/* 4-bit counter */

domain = {

Y[3],
W[3],
@Y[3],

Y[2],
W[2],
@Y[2],

Y[1],
W[1],
@Y[1],

Y[0],
W[0],
@Y[0]

};

/* Init: */
Let S0 = L
Y[3], Y[2], Y[1], Y[0]
.
none_of (
Y[3], Y[2], Y[1], Y[0]
);

let N = L

Y[3], Y[2], Y[1], Y[0],
@Y[3], @Y[2], @Y[1], @Y[0]
.

(@Y[3] == Y[3]  xor Y[2]  & Y[1]  & Y[0] )
&
(@Y[2] == Y[2]  xor Y[1]  & Y[0] )
&
(@Y[1] == Y[1]  xor Y[0] )
&
(@Y[0] == Y[0]')
;

[ Reachable (N, S0) ]
(
Y[3], Y[2], Y[1], Y[0]
)
;
